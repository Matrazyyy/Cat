public class Main {

    public static void main(String[] args) {

        Cat cat1 = new Cat("Tom",4,7);
        Cat cat2 = new Cat("Tosha",3,5);
        Cat cat3 = new Cat("Timosha",6,12);
        System.out.println(cat1);
        System.out.println(cat2);
        System.out.println(cat3);

    }
}
